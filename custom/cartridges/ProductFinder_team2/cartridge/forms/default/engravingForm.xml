<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19%22%3E"/>

    <field formid="message" label="label.profile.message" type="string" binding="message" max-length="50" missing-error="form.firstname.missing" range-error="error.message.50orless" />
    <field formid="search" label="Search" type="string" binding="search" max-length="240" missing-error="form.firstname.missing" range-error="error.message.50orless" />
    <field formid="reviewID" label="label.profile.message" type="string" binding="reviewID" max-length="240" missing-error="form.firstname.missing" range-error="error.message.50orless" />

    <!-- actions provided by the edit shipping address dialog -->
    <action formid="save" valid-form="true"/>
    <action formid="remove" valid-form="false"/>

</form>